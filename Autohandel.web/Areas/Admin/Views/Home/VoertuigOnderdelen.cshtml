
@{
    ViewData["Title"] = "VoertuigOnderdelen";
}
<div class="container">
    <h6 style="margin-top:20px;"><a asp-area="Admin" asp-controller="Home" asp-action="Index" style="margin-top:40px;"><i class="fas fa-arrow-circle-left"></i> Terug naar 'Admin Dashboard'</a></h6>

    <h2 class="text-center text-xl-center text-lowercase text-primary " style="margin-top:25px;">Admin Onderdelen Beheer</h2>

    <div class="row text-center ">

        <div class="col-lg-3 col-md-5 col-sm-8 text-center DashItems ">
            <a asp-controller="CategorieOnderdelen" asp-action="Index" asp-area="Admin">
                <img asp-append-version="true" src="~/images/onderdelen/onderdelenCategorie.png" alt="Onderdeel Categorie" title="Onderdeel Categorie"
                     class="img-fluid DashItemsAfbeeldingen">
            </a>

            <a asp-controller="CategorieOnderdelen" asp-action="Index" asp-area="Admin" class="btn btn-info" style="margin-bottom:2%;">Categorie Onderdelen</a>
        </div>

        <div class=" col-lg-3 col-md-5 col-sm-8   text-center DashItems ">
            <a asp-controller="Home" asp-action="CategorieOnderdelen" asp-area="Admin">
                <img asp-append-version="true" src="~/images/onderdelen/onderdelenProducten.jpg" alt="Producten" title="Producten"
                     class="img-fluid DashItemsAfbeeldingen">
            </a>

            <a asp-controller="Home" asp-action="CategorieOnderdelen" asp-area="Admin" class="btn btn-info" style="margin-bottom:2%;">Producten</a>
        </div>

        <div class="col-lg-3 col-md-5 col-sm-8   text-center DashItems ">
            <a asp-controller="Specificaties" asp-action="Index" asp-area="Admin">
                <img asp-append-version="true" src="~/images/onderdelen/oderdelenSpecificaties.jpg" alt="Specificaties" title="Specificaties"
                     class="img-fluid DashItemsAfbeeldingen">
            </a>

            <a asp-controller="Specificaties" asp-action="Index" asp-area="Admin" class="btn btn-info" style="margin-bottom:2%;">Specificaties</a>
        </div>

    </div>

</div>
